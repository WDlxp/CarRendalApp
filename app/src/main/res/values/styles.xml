<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="CustomAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="actionBarTheme">@style/customActionBar</item>
        <item name="android:statusBarColor">@android:color/white</item>
        <item name="colorPrimary">@android:color/holo_blue_light</item>
    </style>
    <style name="customActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@android:color/white</item>
    </style>
    <style name="customLoginActivityTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="colorPrimaryDark">@android:color/white</item>
    </style>

</resources>
